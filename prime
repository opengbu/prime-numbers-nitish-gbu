#include <iostream>
#include<math.h>
using namespace std;

int prime(int);

int main()
{
   bool isPrime;
   int m;
   cout<<"Please enter the number till you want to check prime "<<endl;
   cin>>m;
   for(int n = 2; n < m; n++) 
   {
   	
      isPrime = prime(n);

      if(isPrime == true)
         cout<<n<<" "<<endl;
   }
   return 0;
}
int prime(int n)
 {
   bool isPrime = true;

   for(int i = 2; i <= sqrt(n); i++)
    {
   	
      if (n%i == 0) {
         isPrime = false;
         break;
      }
   }  
   return isPrime;
}
